/*up arrrow code*/

.arrow_box {
	width: 250px;
	height: auto;
	padding: 11px 10px 10px 0;
	border-radius: 20px;
	text-align: left;
	position: abolute;
	background: rgba(255,255,255,0.9);
	border: 1px solid #000;
	font-family: 'Avenir-Medium';
	font-size: 18px;
	color: #000;
	display: none;
	line-height: 30px;
	position: absolute;
	right: -5px;
	margin-top: 15px;
	z-index: 2;
}
.arrow_box ul li {
	list-style: none;
	color: #000;
}
.arrow_box.one a {
	color: #000;
}
.arrow_box:after, .arrow_box:before {
	bottom: 70%;
	left: 90%;
	border: 1px solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;
}
.arrow_box:after {
	border-color: rgba(136,183,213, 0);
	border-bottom-color: rgba(136,183,213,0);
	margin-left: 0;
}
.arrow_box:before {
	border-color: rgba(194,225,245,0);
	border-bottom-color: rgba(255,255,255,0.9);
	margin-left: 0;
}
