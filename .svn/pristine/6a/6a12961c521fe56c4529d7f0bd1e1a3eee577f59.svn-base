/* nDorsed page css */
.stylish-input-group .input-group-addon{
    background: white !important; 
}
.stylish-input-group .form-control{
	border-right:0; 
	box-shadow:0 0 0; 
	border-color:#ccc;
}
.stylish-input-group button{
    border:0;
    background:transparent;
}
/* Search date css */
h4.search-date{
	color:#fff;
	margin-left: 15px;
}
.mt15{
	margin:15px 0;
}
.mt30{
	margin:30px 0;
}
.border-bottom{
	border-bottom:solid 1px #fff;
	opacity:0.3;
	width:100%;
}
.media-user{
	background:url(../img/user.png);
	background-repeat:no-repeat;
	background-position:center center;
	width:100px;
	height:100px;
	border:0;
}
.media-user img {
	border:0;
	outline:0;
}
.user-review{
	color:#f7b4b6;
	font-size:20px;
	text-align:center;
}
.name{
	color:#fff;
	margin-top:15px;
	margin-left:25px;
}
.name-right{
	color:#fff;
	margin-top:15px;
	margin-right:25px;
	float:right;
}
.yellow{
	color:#fdf5b0;
}
.grass-green{
	color:#afd366;
}
.light-blue{
	color:#bab2d8;
}
.green-light{
	color:#aadbcf;
}
.hardworking{
	color:#91a7d6;
}
.calm{
	color:#e9eb94;
}
.awesome{
	color:#9ebce2;
}
.yellow-bg{
	width:100%;
	background-color:#f37522;
	color:#fff;
	font-size:18px;
	height:42px;
	padding:10px;
	margin-top:30px;
}
.blue-bg{
	background-color:#1f3262;
	border: solid 1px rgba(255,255,255,0.3);
	width:100%;
	padding:20px 20px 0 20px;
	margin-top:15px;
}
/* nDorsement detail page css */
.grey-bg{
	background-color:#d3d3d3;
	border: solid 1px rgba(255,255,255,0.3);
	width:100%;
	padding:20px 20px 0 20px;
	margin-top:15px;
	height:auto;
	color:#000;
}
.rohan-space{
	margin:0 10px;
	font-size:20px;
	font-weight:600;
}
.date-detail{
	font-size:20px;
	text-align:center;

}
.ndorser-detail{
	margin-top:5px;
	margin-left:15px;
}
.ndorsed-detail{
	margin-top:5px;
	margin-right:15px;
}
.core-value{
	font-size:20px;
	background-color:#fff;
	padding:20px;
}
.hi-cehck{
	font-size:20px;
	padding:20px;
	color:#fff;
}