            var $ = jQuery.noConflict();
            $(document).ready(function ($) {
                "use strict";
                $(".bannersize").click(function (event) {
                    $("#popupsize").val($(this).find("input").val());
                });
                $('.carousel').carousel({
                    interval: 3000,
                    pause: false
                })
                $(".hiw").click(function (event) {
                    event.preventDefault();
                    navigate('how-it-works-section');
                });
                $(".rew").click(function (event) {
                    event.preventDefault();
                    navigate('review-section');
                });
                $(".gstarted").click(function (event) {
                    event.preventDefault();
                    navigate('sign-up');
                });
            });
            $(window).load(function () {
                $(".item").each(function (index, el) {
                    setTimeout(function () {
                        $(el).removeClass('blur');
                    }, 2000);
                });
                setTimeout(function () {
                    $('.blur').removeClass('blur');
                }, 2000);
            });
            function navigate(divId) {
                if (md.mobile() != null) {
                    $('html, body').animate({scrollTop: $("#" + divId).offset().top-30}, 700);
                } else {
                    $('html, body').animate({scrollTop: $("#" + divId).offset().top}, 700);
                }
            }
            ;
            function isFormVisible() {
                var viewData = getViewportOffset($("#new_user"));
                return  ("insideViewport" in viewData && viewData.insideViewport == true) ? true : false;
            }

            function isNavBarVisible() {
                var viewData = getViewportOffset($(".navbar-inverse"));
                return  ("insideViewport" in viewData && viewData.insideViewport == true) ? true : false;
            }

            function isButtonNewVisible() {
                var viewData = getViewportOffset($("#carousel-background1"));
                return  ("insideViewport" in viewData && viewData.insideViewport == true) ? true : false;
            }

            function isButtonVisible() {
                if ($("#carousel-background .ptc1").is(":visible") == true) {
                    var viewData = getViewportOffset($("#carousel-background .ptc1"));
                } else {
                    var viewData = getViewportOffset($("#carousel-background .ptc2"));
                }
                return  ("insideViewport" in viewData && viewData.insideViewport == true) ? true : false;
            }

            $(document).scroll(function () {
                if (($("#carousel-background1").is(":visible") == true) && (isButtonNewVisible() == true)) {
                    $("body").toggleClass("global-nav-fixed", false);
                } else if (($("#carousel-background").is(":visible") == true) && (isButtonVisible() == true)) {
                    $("body").toggleClass("global-nav-fixed", false);
                } else {
                    $("body").toggleClass("global-nav-fixed", true);
                }
            })

            function getViewportOffset($e) {
                var $window = $(window), scrollLeft = $window.scrollLeft(), scrollTop = $window.scrollTop(), offset = $e.offset(),
                        rect1 = {x1: scrollLeft, y1: scrollTop, x2: scrollLeft + $window.width(), y2: scrollTop + $window.height()},
                rect2 = {x1: offset.left, y1: offset.top, x2: offset.left + $e.width(), y2: offset.top + $e.height()};
                return {
                    left: offset.left - scrollLeft,
                    top: offset.top - scrollTop,
                    insideViewport: rect1.x1 < rect2.x2 && rect1.x2 > rect2.x1 && rect1.y1 < rect2.y2 && rect1.y2 > rect2.y1
                };
            }
