        <script>
            $(".footer__toggle").click(function () {
                var t;
                return t = void 0, $(".footer__content").slideToggle("fast"), t = $(window).height() - 225, $("html, body").animate({
                    scrollTop: $(".footer__content").offset().top - t
                }, 500), $(".togglable-footer").toggleClass("is-open"), $(".footer__header").toggleClass("is-open"), $(".footer__social").toggleClass("is-open"), !1
            }), $(".style-tips-toggle").click(function () {
                return $(".footer__styletips__select--big").slideToggle("fast"), $(".footer__styletips__select--small").slideToggle("fast"), $(".nav__style-tips").toggleClass("is-open"), $(".footer__styletips__select").toggleClass("is-open"), !1
            }), $(".twitter-link-js").click(function () {
                return $(".footer__social").toggleClass("twitter-is-open"), $(".footer__social").removeClass("facebook-is-open"), $(".footer__social").removeClass("pinterest-is-open"), $(".footer__social").removeClass("instagram-is-open"), $(".footer__social__select--twitter").slideToggle("fast"), $(".footer__social__select--facebook").slideUp(200), $(".footer__social__select--pinterest").slideUp(200), $(".footer__social__select--instagram").slideUp(200), !1
            }), $(".facebook-link-js").click(function () {
                return $(".footer__social").toggleClass("facebook-is-open"), $(".footer__social").removeClass("twitter-is-open"), $(".footer__social").removeClass("pinterest-is-open"), $(".footer__social").removeClass("instagram-is-open"), $(".footer__social__select--facebook").slideToggle("fast"), $(".footer__social__select--twitter").slideUp(200), $(".footer__social__select--instagram").slideUp(200), $(".footer__social__select--pinterest").slideUp(200), !1
            }), $(".pinterest-link-js").click(function () {
                return $(".footer__social").toggleClass("pinterest-is-open"), $(".footer__social").removeClass("twitter-is-open"), $(".footer__social").removeClass("facebook-is-open"), $(".footer__social").removeClass("instagram-is-open"), $(".footer__social__select--pinterest").slideToggle("fast"), $(".footer__social__select--twitter").slideUp(200), $(".footer__social__select--facebook").slideUp(200), $(".footer__social__select--instagram").slideUp(200), !1
            }), $(".instagram-link-js").click(function () {
                return $(".footer__social").toggleClass("instagram-is-open"), $(".footer__social").removeClass("twitter-is-open"), $(".footer__social").removeClass("facebook-is-open"), $(".footer__social").removeClass("pinterest-is-open"), $(".footer__social__select--instagram").slideToggle("fast"), $(".footer__social__select--twitter").slideUp(200), $(".footer__social__select--facebook").slideUp(200), $(".footer__social__select--pinterest").slideUp(200), !1
            })

            $(".link--cookie").hover(function () {
                $(".cookie-nav").slideToggle("fast")
            })

            $("#toggle").click(function () {
                $(".hamburger").toggleClass("active"),
                        $("nav.main").toggleClass("visible")
            })

            $("#getStart-pop").click(function (event) {
                event.preventDefault()
                $("body").addClass("modal-open"),
                        $("#getStart-popup").fadeIn();
            })

            $("button.modal-close").click(function () {
                $("body").removeClass("modal-open"),
                        $("#getStart-popup").fadeOut();
            })

            $("#sizeChart-popup").click(function () {
                $("body").addClass("modal-open"),
                        $("#sizeChart-pop").fadeIn();
            })

            $("button.modal-close").click(function () {
                $("body").removeClass("modal-open"),
                        $("#sizeChart-pop").fadeOut();
            })

