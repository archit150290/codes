                                            <input type="radio" name="user[client_attributes][scalar_attributes_attributes][dress_generic_size_id]" id="user_client_attributes_scalar_attributes_attributes_dress_generic_size_id_40" value="40" class="form-control rbtn-scale" />
                                            <span for="user_client_attributes_scalar_attributes_attributes_dress_generic_size_id_40">L</span>        
                                        </label>
                                        <label class="btn btn-default">
                                            <input type="radio" name="user[client_attributes][scalar_attributes_attributes][dress_generic_size_id]" id="user_client_attributes_scalar_attributes_attributes_dress_generic_size_id_41" value="41" class="form-control rbtn-scale" />
                                            <span for="user_client_attributes_scalar_attributes_attributes_dress_generic_size_id_41">XL</span>        
                                        </label>
