        <script>
            var $ = jQuery.noConflict();
            $(document).ready(function ($) {
                "use strict";
                $(".bannersize").click(function (event) {
                    $("#popupsize").val($(this).find("input").val());
                });
                
                //============function for slider
                var t;
                t = setTimeout(function () {
                    $('.carousel').carousel({interval: 1500, pause: false});
                    clearTimeout(t);
                }, 0);

                $('.carousel').on('slid.bs.carousel', function () {
                    clearTimeout(t);
                    var duration = 3000;
                    $('.carousel').carousel('pause');
                    if($(".carousel-inner .casual").hasClass("active") == true){
                        duration = 1500;
                    }
                    t = setTimeout(function () {
                        $('.carousel').carousel({pause: false});
                    }, duration - 1000);
                });
                //============function for slider
                $(".hiw").click(function (event) {
                    event.preventDefault();
                    navigate('how-it-works-section');
                });
                $(".rew").click(function (event) {
                    event.preventDefault();
                    navigate('review-section');
                });
                $(".gstarted").click(function (event) {
                    event.preventDefault();
                    navigate('sign-up-pos');
                });

                $('#getStart-dialog').modal('')
                $(document).on("click", "#close-promo", function () {
                    $("body").removeClass("promos");
                    ampt.click('top-promo-close');
                    document.cookie = "top-promo-hidden=true;";
                })
                $("#call-to-action").click(function () {
                    ampt.click('top-promo');
                })
                if (getCookie("top-promo-hidden") == "") {
                    $("body").addClass("promos");
                }
                //=========if esc key is pressed
                $(document).on('keyup', function (evt) {
                    if (evt.keyCode == 27 && $("#sizeChart-pop , #getStart-popup").is(":visible") == true) {
                        $("#sizeChart-pop, #getStart-popup").fadeOut()
                        $("body").removeClass("modal-open")
                    }
                });

                $(document).on("click", "body", function (event) {
                    if ($("#sizeChart-pop , #getStart-popup").is(":visible") == true) {
                        if (event.target.id == "sizeChart-pop" || event.target.id == "getStart-popup") {
                            $("#sizeChart-pop, #getStart-popup").fadeOut()
                            $("body").removeClass("modal-open")
                        }
                    }
                });

            });

            $(window).load(function () {

                $(".item").each(function (index, el) {
                    setTimeout(function () {
                        $(el).removeClass('blur');
                    }, 2000);
                });
                setTimeout(function () {
                    $('.blur').removeClass('blur');
                }, 2000);
            });

            function navigate(divId) {
                if (md.mobile() != null) {
                    var nheight = 30
                    if ($('#top-promo').is(':visible')) {
                        nheight = 190
                    }
                    $('html, body').animate({scrollTop: $("#" + divId).offset().top - nheight}, 700);
                } else {
                    $('html, body').animate({scrollTop: $("#" + divId).offset().top}, 700);
                }
            }
            ;
            function getRandomInt(min, max) {
                return Math.floor(Math.random() * (max - min)) + min;
            }

            /**/
            $(function () {
                function t() {
                    return /(iPhone).*OS [1-5](.*) like Mac OS X/i.test(navigator.userAgent) || /(iPod).*OS [1-6](.*) like Mac OS X/i.test(navigator.userAgent) || /(iPad).*OS [1-4](.*) like Mac OS X/i.test(navigator.userAgent)
                }
                if ($("body.public-home").length) {
                    var e = function () {
                        var t = $("body").offset().top + 500,
                                n = $(document).scrollTop();
                        var showMe = n > t;
                    };
                    setTimeout(e, 10), setTimeout(function () {
                        t() ? $("body").addClass("no-fixed-background") : ($("body.public-home .wrapper").append('<section class="bg"></section>'), $("body").addClass("fixed-background"))
                    }, 1000)
                }
            });

            $(function () {
                return $("[ data-slow-scroll-via-class ]").click(function (t) {
                    var e, n, i;
                    return n = $(this).attr("href").replace(/.*#/, "."), n += ":visible", 0 !== $(n).length ? (t.preventDefault(), i = $(n).offset().top, e = $(this).data("slow-scroll"), e && (i += e), $("html,body").animate({
                        scrollTop: i
                    }, 700), !1) : void 0
                })
            })
            $(".hamburger, .mobile-global-nav-tray li a").click(function () {
                $("body").toggleClass("mobile-navbar-shown");
            })

