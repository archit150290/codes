            $(".size-chart").on("click", function () {
                var sizechartclass = $(this).attr("class").split(" ")[1];
                if (typeof (sizechartclass) != "undefined") {
                    $("table.size-chart tbody").attr("data-whichsc", "")
                    $("table.size-chart tbody").attr("data-whichsc", sizechartclass)
                }
            })

